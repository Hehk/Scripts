{
  "name": "scripts",
  "version": "1.0.0",
  "description": "My scripts",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "ts-node build.ts",
    "format": "prettier --write .",
    "ts": "tsc --noEmit -w",
