  "devDependencies": {
    "@types/node": "^16.11.4",
    "esbuild": "^0.13.8",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "node-fetch": "^3.0.0",
    "puppeteer-core": "^10.4.0"
  }
}
